{
  "dashboard": {
    "id": null,
    "title": "AutoGen Code Review Bot Dashboard",
    "tags": ["autogen", "code-review", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"autogen-bot\"}",
            "legendFormat": "Bot Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"autogen-bot\"}[5m])",
            "legendFormat": "{{status}} requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"autogen-bot\"}[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 18, "y": 0}
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"autogen-bot\"} / 1024 / 1024",
            "legendFormat": "Memory (MB)"
          }
        ],
        "yAxes": [
          {
            "label": "MB",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "GitHub API Rate Limit",
        "type": "graph",
        "targets": [
          {
            "expr": "github_rate_limit_remaining{job=\"autogen-bot\"}",
            "legendFormat": "Remaining Requests"
          },
          {
            "expr": "github_rate_limit_limit{job=\"autogen-bot\"}",
            "legendFormat": "Limit"
          }
        ],
        "yAxes": [
          {
            "label": "Requests",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Review Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "review_queue_size{job=\"autogen-bot\"}",
            "legendFormat": "Queue Size"
          }
        ],
        "yAxes": [
          {
            "label": "Items",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(cache_hits_total{job=\"autogen-bot\"}[5m]) / (rate(cache_hits_total{job=\"autogen-bot\"}[5m]) + rate(cache_misses_total{job=\"autogen-bot\"}[5m])) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Agent Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(agent_conversation_duration_seconds_sum{job=\"autogen-bot\"}[5m]) / rate(agent_conversation_duration_seconds_count{job=\"autogen-bot\"}[5m])",
            "legendFormat": "{{agent_type}} avg duration"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=\"autogen-bot\"}, instance)",
          "current": {
            "value": "autogen-bot"
          }
        }
      ]
    }
  }
}